<?xml version="1.0" encoding="UTF-8" ?>
<system xmlns="http://david.moten.org/xuml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://david.moten.org/xuml ../../../../../../xuml-jaxb/src/main/resources/xuml.xsd">

	<class name="Domain" domain="xuml">
		<attribute name="Name" type="String">
			<identifier number="1" />
		</attribute>
		<attribute name="Alias" type="String">
			<identifier number="2" />
		</attribute>
	</class>

	<class name="Class" domain="xuml">
		<attribute name="Name" type="String">
			<identifier number="1" />
		</attribute>
		<referentialAttribute name="Domain" otherName="Name">
			<identifier number="1" />
			<identifier number="2" />
			<identifier number="3" />
			<identifier number="4" />
			<reference relationship="14" />
		</referentialAttribute>
		<!-- <referentialAttribute name="Element"> <identifier number="2" /> <reference 
			relationship="14" /> </referentialAttribute> -->
		<attribute name="Cnum" type="String">
			<identifier number="3" />
		</attribute>
		<attribute name="Alias" type="String">
			<identifier number="4" />
		</attribute>
		<operation name="validate" />
		<state name="Valid" number="1" />
		<state name="Invalid" number="2">
			<onEntry event="ValidationError">
				<action>do something here</action>
			</onEntry>
		</state>
		<event name="ValidationError">
			<parameter name="message" type="String" />
		</event>
		<transition from="Valid" to="Invalid" event="ValidationError" />
	</class>

	<class name="Attribute" domain="xuml">
		<attribute name="Name" type="String">
			<identifier number="1" />
		</attribute>
		<referentialAttribute name="Class" otherName="Name">
			<identifier number="1" />
			<reference relationship="20" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<reference relationship="20" />
		</referentialAttribute>
	</class>

	<class name="ReferentialAttribute" domain="xuml">
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<referentialAttribute name="Class">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<derivedAttribute name="Type" type="String"
			formula="class + '.' + domain">
			<attribute name="Class" />
			<attribute name="Domain" />
		</derivedAttribute>
	</class>

	<class name="SpecializedClass" domain="xuml">
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="23" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="23" />
		</referentialAttribute>
	</class>

	<class name="NonSpecializedClass" domain="xuml">
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="23" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="23" />
		</referentialAttribute>
	</class>

	<class name="Identifier" domain="xuml">
		<attribute name="Number" type="String">
			<identifier number="1" />
		</attribute>
		<referentialAttribute name="Class" otherName="Name">
			<identifier number="1" />
			<reference relationship="27" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<reference relationship="27" />
		</referentialAttribute>
	</class>

	<class name="IdentifierAttribute" domain="xuml" relationship="22">
		<referentialAttribute name="Identifier"
			otherName="Number">
			<identifier number="1" />
			<associativeReference relationship="22" side="1" />
		</referentialAttribute>
		<referentialAttribute name="Attribute" otherName="Name">
			<identifier number="1" />
			<associativeReference relationship="22" side="2" />
		</referentialAttribute>
		<referentialAttribute name="Class">
			<identifier number="1" />
			<associativeReference relationship="22" side="1" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<associativeReference relationship="22" side="1" />
		</referentialAttribute>
	</class>

	<class name="NativeAttribute" domain="xuml">
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<referentialAttribute name="Class">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="21" />
		</referentialAttribute>
		<referentialAttribute name="Type" otherName="Name">
			<reference relationship="24" />
		</referentialAttribute>
	</class>

	<class name="Type" domain="xuml">
		<attribute name="Name" type="String">
			<identifier number="1" />
		</attribute>
	</class>

	<class name="AttributeInDerivation" domain="xuml" relationship="26">
		<referentialAttribute name="SourceAttribute"
			otherName="Name">
			<identifier number="1" />
			<associativeReference relationship="26" side="1" />
		</referentialAttribute>
		<referentialAttribute name="SourceClass"
			otherName="Class">
			<identifier number="1" />
			<associativeReference relationship="26" side="1" />
		</referentialAttribute>
		<referentialAttribute name="TargetAttribute"
			otherName="Name">
			<identifier number="1" />
			<associativeReference relationship="26" side="2" />
		</referentialAttribute>
		<referentialAttribute name="TargetClass"
			otherName="Class">
			<identifier number="1" />
			<associativeReference relationship="26" side="2" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<associativeReference relationship="26" side="1" />
		</referentialAttribute>
	</class>

	<class name="IndependentAttribute" domain="xuml">
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
		<referentialAttribute name="Class">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
	</class>

	<class name="DerivedAttribute" domain="xuml" >
		<referentialAttribute name="Name">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
		<referentialAttribute name="Class">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
		<referentialAttribute name="Domain">
			<identifier number="1" />
			<superclassReference relationship="25" />
		</referentialAttribute>
		<attribute name="Formula" type="String" />
	</class>

	<association domain="xuml" number="14">
		<class1 name="Class" verbClause="models" multiplicity="MANY" />
		<class2 name="Domain" verbClause="is modelled in" multiplicity="ONE" />
	</association>

	<association domain="xuml" number="20">
		<class1 name="Class" verbClause="characterizes" multiplicity="ONE" />
		<class2 name="Attribute" verbClause="is characterized by"
			multiplicity="ONE_MANY" />
	</association>

	<association domain="xuml" number="22">
		<class1 name="Identifier" verbClause="is required in"
			multiplicity="MANY" />
		<class2 name="Attribute" verbClause="requires" multiplicity="ONE_MANY" />
	</association>

	<association domain="xuml" number="26">
		<class1 name="Attribute" verbClause="is derived from"
			multiplicity="ONE_MANY" />
		<class2 name="DerivedAttribute" verbClause="contributes to the value of"
			multiplicity="MANY" />
	</association>

	<association domain="xuml" number="27">
		<class1 name="Class" verbClause="uniquely distinguishes"
			multiplicity="ONE" />
		<class2 name="Identifier" verbClause="is uniquely distinguished by"
			multiplicity="ONE_MANY" />
	</association>

	<association domain="xuml" number="24">
		<class1 name="NativeAttribute" verbClause="classifies"
			multiplicity="MANY" />
		<class2 name="Type" verbClause="is classified by" multiplicity="ONE" />
	</association>

	<generalization domain="xuml" number="21" superclass="Attribute"
		subclass="ReferentialAttribute" />

	<generalization domain="xuml" number="21" superclass="Attribute"
		subclass="NativeAttribute" />

	<generalization domain="xuml" number="25" superclass="NativeAttribute"
		subclass="DerivedAttribute" />

	<generalization domain="xuml" number="25" superclass="NativeAttribute"
		subclass="IndependentAttribute" />

	<generalization domain="xuml" number="23" superclass="Class"
		subclass="SpecializedClass" />

	<generalization domain="xuml" number="23" superclass="Class"
		subclass="NonSpecializedClass" />

</system>