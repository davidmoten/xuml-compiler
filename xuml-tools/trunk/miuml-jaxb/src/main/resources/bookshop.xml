<?xml version="1.0" encoding="UTF-8" ?>
<Domains xmlns="http://www.miuml.org/metamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.miuml.org/metamodel miuml-metamodel.xsd">

	<SymbolicType Name="String" MinLength="1" MaxLength="255"
		Prefix="" Suffix="" ValidationPattern="?.*" DefaultValue="" />
	<IntegerType Name="Nominal" Units="None" DefaultValue="0"
		UpperLimit="99999999999" LowerLimit="1" />

	<ModeledDomain Name="Shop" Alias="Shop">

		<Subsystem Name="Bookshop" Alias="Bookshop" Floor="1"
			Ceiling="99">

			<NonSpecializedClass Name="Book" Cnum="1" Number="1" Alias="Book">
				<IndependentAttribute Name="isbn" Type="String">
					<Identifier Number="1" />
				</IndependentAttribute>
				<IndependentAttribute Name="Title" Type="String" />
			</NonSpecializedClass>

			<NonSpecializedClass Name="Publisher" Cnum="2" Number="2" Alias="Author">
				<IndependentAttribute Name="Id" Type="Nominal">
					<Identifier Number="1" />
				</IndependentAttribute>
				<IndependentAttribute Name="Name" Type="String" />
			</NonSpecializedClass>

			<BinaryAssociation Number="2" Rnum="1">
				<ActivePerspective OnePerspective="false"
					Conditional="false" ViewedClass="Publisher" Phrase="published" />
				<PassivePerspective OnePerspective="false"
					Conditional="false" ViewedClass="Book" Phrase="is published by" />
			</BinaryAssociation>

		</Subsystem>

	</ModeledDomain>

</Domains>